int main(int argc, char* argv[])
{
	int n,
	l,
	u,
	M,        //server amount
	m;        //available
	n = 15;
	l = 3;
	mu = 2;
	M = 5;
	m = M;

	Customer cust;
	float PQ[n*2];
	cust->arrivalTime = 00.00;
	cust->arrival = 1;

	return 0;
}


ProcessNextEvent(Customer cust)
{
	if(cust.arrival == 1)
	{
		if(m > 0)
		{
			m--;
			cust->StartofService = cust->arrivalTime;
		}
	}
}
