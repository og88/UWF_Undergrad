#include <stdio.h>
#include <pthread.h>
#include <time.h>
#include "Collatz_computations.c"


int main(int argc, char *argv[])
{
	clock_t begin = clock();
	int N;
	buildarray();
	N = 100;
	compare(N);
	clock_t end = clock();
	double time_spent = (double)(end - begin) / CLOCKS_PER_SEC;
	printarray();
	printf("%f\n",time_spent);

}


