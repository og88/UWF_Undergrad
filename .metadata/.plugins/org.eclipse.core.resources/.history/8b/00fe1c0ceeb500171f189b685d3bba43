#include <pthread.h>
#include <stdio.h>
#include <time.h>
#include "Collatz_computations.c"


int main(int argc, char *argv[])
{
	int T,t;
	T = 5;
	pthread_t threads[T];
	buildarray();
	N = argv[1];
	printf("%i", N);
	clock_t begin = clock();
	for(t = 0; t < T; t++)
	{
		pthread_create(&threads[t], NULL, compare, NULL);
	}
	for(t=0; t<T; t++){
		pthread_join(threads[t], NULL);
	}
	clock_t end = clock();
	double time_spent = (double)(end - begin) / CLOCKS_PER_SEC;
	printf("%f\n",time_spent);
	//printarray();
	pthread_exit(NULL);
}


